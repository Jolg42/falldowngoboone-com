{
  "name": "falldowngoboone-com",
  "version": "1.0.0",
  "description": "My personal website",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run watch:ssg & npm run watch:css",
    "build": "npm run clean && npm run build:ssg & npm run build:css",
    "build:prod": "npm run build:ssg & npm run build:css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf _site",
    "build:css": "sass src/scss:_site/css",
    "build:ssg": "eleventy",
    "watch:css": "npm run build:css -- --watch",
    "watch:ssg": "npm run build:ssg -- --serve --port=3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falldowngoboone/falldowngoboone-com.git"
