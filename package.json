  },
  "homepage": "https://github.com/falldowngoboone/falldowngoboone-com#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "luxon": "^1.25.0",
    "reading-time": "^1.2.0",
    "sass": "^1.26.10"
  }
}
